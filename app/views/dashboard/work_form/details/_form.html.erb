<%= form_with(model: @work_version, url: url, local: true) do |form| %>

  <%= render 'dashboard/work_form/shared/form_error_message',
             form: form,
             heading: t('dashboard.work_versions.edit.error_message', error: pluralize(work_version.errors.count, 'error')) %>

  <%= render 'fields', form: form %>

  <%= render 'dashboard/work_form/shared/action_footer', form: form, publish: false %>
<% end %>
